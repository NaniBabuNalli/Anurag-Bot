{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": false,
  "outputDirectory": ".",
  "installCommand": false,
  "framework": null,
  "public": true,
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key"
  }
}